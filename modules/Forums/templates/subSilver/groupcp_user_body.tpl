
<table width="100%" cellspacing="2" cellpadding="2" border="0" align="center">
  <tr> 
	<td align="left"><span class="nav"><a href="{U_INDEX}" class="nav">{L_INDEX}</a></span></td>
  </tr>
</table>

<table width="100%" cellpadding="2" cellspacing="1" border="0" class="forumline">
  <!-- BEGIN joined -->
  <tr> 
	<th colspan="3" align="center" class="thHead" height="25">{L_GROUP_MEMBERSHIP_DETAILS}</th>
  </tr>
  <!-- BEGIN member -->
  <tr> 
	<td class="catLeft" height="28" colspan="2"><span class="cattitle">{L_YOU_BELONG_GROUPS}</span></td>
	<td class="rowpic" align="right" width="200">&nbsp;</td>
  </tr>
	  <!-- BEGIN grouprow -->
	  <tr>
		<td class="row1" align="center" valign="middle" height="50" width="5%"><img src="images/groups/groups.gif" width="46" height="25" alt="{joined.member.grouprow.GROUP_NAME}" title="{joined.member.grouprow.GROUP_NAME}" /></td>
	   <td class="row1" height="50">
		<span class="forumlink">
			<a href="{joined.member.grouprow.U_GROUP}" class="forumlink">{joined.member.grouprow.GROUP_NAME}</a><br />
		  </span> <span class="genmed">{joined.member.grouprow.GROUP_DESC}
		  </span>
	   </td>
	<td class="row2" align="center" valign="middle" height="50" nowrap="nowrap"> <span class="gensmall">{joined.member.grouprow.GROUP_TYPE}</span></td>
	  </tr>
	  <!-- END grouprow -->
  <!-- END member -->
  <!-- BEGIN pending -->
  <tr> 
	<td class="catLeft" height="28" colspan="2"><span class="cattitle">{L_PENDING_GROUPS}</span></td>
	<td class="rowpic" align="right">&nbsp;</td>
  </tr>
      <!-- BEGIN grouprow -->
	  <tr>
		<td class="row1" align="center" valign="middle" height="50" width="5%"><img src="images/groups/groups.gif" width="46" height="25" alt="{joined.pending.grouprow.GROUP_NAME}" title="{joined.pending.grouprow.GROUP_NAME}" /></td>
	   <td class="row1" height="50"><span class="forumlink">
 			<a href="{joined.pending.grouprow.U_GROUP}" class="forumlink">{joined.pending.grouprow.GROUP_NAME}</a><br />
		  </span> <span class="genmed">{joined.pending.grouprow.GROUP_DESC}
		  </span>
	   </td>
	<td class="row2" align="center" valign="middle" height="50" nowrap="nowrap"> <span class="gensmall">{joined.pending.grouprow.GROUP_TYPE}</span></td>
	  </tr>
      <!-- END grouprow -->
  <!-- END pending -->
  <!-- END joined -->
  <!-- BEGIN remaining -->
  <tr>
   <th colspan="3" align="center" class="thHead" height="25">{L_JOIN_A_GROUP}</th>
  </tr>
  <tr> 
	<td class="catLeft" height="28" colspan="2"><span class="cattitle">{L_SELECT_A_GROUP}</span></td>
	<td class="rowpic" align="right">&nbsp;</td>
  </tr>
      <!-- BEGIN grouprow -->
	  <tr>
		<td class="row1" align="center" valign="middle" height="50" width="5%"><img src="images/groups/groups.gif" width="46" height="25" alt="{remaining.grouprow.GROUP_NAME}" title="{remaining.grouprow.GROUP_NAME}" /></td>
	   <td class="row1" height="50"><span class="forumlink">
 			<a href="{remaining.grouprow.U_GROUP}" class="forumlink">{remaining.grouprow.GROUP_NAME}</a><br />
		  </span> <span class="genmed">{remaining.grouprow.GROUP_DESC}
		  </span>
	   </td>
	<td class="row2" align="center" valign="middle" height="50" nowrap="nowrap"> <span class="gensmall">{remaining.grouprow.GROUP_TYPE}</span></td>
	  </tr>
      <!-- END grouprow -->
  <!-- END remaining -->
</table>

<table width="100%" cellspacing="2" border="0" align="center" cellpadding="2">
  <tr> 
	<td align="right" valign="top"><span class="gensmall">{S_TIMEZONE}</span></td>
  </tr>
</table>

<br clear="all" />

<table width="100%" cellspacing="2" border="0" align="center">
  <tr> 
	<td valign="top" align="right">{JUMPBOX}</td>
  </tr>
</table>
