<h1 align="center">{HEADER}</h1>

<p align="center" class="genmed">{CURRENT_TIME}</p>

<form action="{SQL_ACTION}" method="post">

<table cellspacing="0" cellpadding="0" border="0" align="center" width="100">
  <tr>
    <td align="center"><span class="gen"><strong>{QUERY_TITLE}</strong></span></td>
  </tr>
  <tr>
    <td><textarea name="this_query" rows="10" cols="100" class="post"></textarea></td>
  </tr>
  <tr>
    <td align="center">
      <input type="submit" name="repairall" value="{REPAIR_ALL_BUTTON}" class="liteoption" />&nbsp;<input type="submit" name="submit" value="{SUBMIT_BUTTON}" class="liteoption" />&nbsp;<input type="submit" name="optimizeall" value="{OPTIMIZE_ALL_BUTTON}" class="liteoption" />
    </td>
  </tr>
  <tr>
    <td align="center">
      <br />
      {L_WITH_SELECTED_WORD}
      <select name="with_selected">
        <option value=optimize>{L_WITH_SELECTED_OPTIMIZE}
        <option value=repair>{L_WITH_SELECTED_REPAIR}
        <option value=empty>{L_WITH_SELECTED_EMPTY}
        <option value=drop>{L_WITH_SELECTED_DROP}
      </select>
      <input type="submit" name="go_with_selected" value="{SUBMIT_BUTTON}" class="liteoption" />
    </td>
  </tr>
</table>

<br />

<!-- BEGIN switch_submit_result -->
<table width="100%" cellspacing="0" cellpadding="0" border="1" class="forumline">
  <tr>
    <td colspan="{SUBMIT_RESULT_FIELD_COUNT}" align="left" class="catLeft">
      <span class="gen">{SUBMIT_RESULT_QUERY}</span>
    </td>
  </tr>
  <tr>
    <!-- BEGIN submit_result_fields -->
    <td align="center" class="row1"><span class="genmed">{switch_submit_result.submit_result_fields.SUBMIT_RESULT_FIELD_NAME}</span></td>
    <!-- END submit_result_fields -->
  </tr>
  <!-- BEGIN submit_result_data -->
  <tr>
    <!-- BEGIN submit_result_data_row -->
    <td align="center" class="row1"><span class="genmed">{switch_submit_result.submit_result_data.submit_result_data_row.SUBMIT_RESULT_DATA}</span></td>
    <!-- END submit_result_data_row -->
  </tr>
  <!-- END submit_result_data -->
</table>
<br />
<!-- END switch_submit_result -->

<!-- BEGIN switch_table_browse -->
<table width="100%" cellspacing="0" cellpadding="0" border="1" class="forumline">
  <!-- BEGIN table_browse_menu -->
  <tr>
    <td colspan="{switch_table_browse.table_browse_menu.BROWSE_MENU_COLSPAN}" align="left" class="catLeft">
      <span class="gen"><a href="{switch_table_browse.table_browse_menu.FIRST_PAGE}">{switch_table_browse.table_browse_menu.L_FIRST_PAGE}</a>&nbsp;</span>
      <span class="gen"><a href="{switch_table_browse.table_browse_menu.NEXT_PAGE}">{switch_table_browse.table_browse_menu.L_NEXT_PAGE}</a>&nbsp;</span>
      <span class="gen"><a href="{switch_table_browse.table_browse_menu.PREVIOUS_PAGE}">{switch_table_browse.table_browse_menu.L_PREVIOUS_PAGE}</a>&nbsp;</span>
      <span class="gen"><a href="{switch_table_browse.table_browse_menu.SORT_ASC}">{switch_table_browse.table_browse_menu.L_SORT_ASC}</a>&nbsp;</span>
      <span class="gen"><a href="{switch_table_browse.table_browse_menu.SORT_DESC}">{switch_table_browse.table_browse_menu.L_SORT_DESC}</a>&nbsp;</span>
    </td>
  </tr>
  <!-- END table_browse_menu -->
  <tr>
    <td class="row1">&nbsp;</td>
    <!-- BEGIN table_browse_fields -->
    <td class="row1"><span class="genmed"><strong><a href="{switch_table_browse.table_browse_fields.TABLE_BROWSE_FIELD_ORDER}">{switch_table_browse.table_browse_fields.TABLE_BROWSE_FIELD_NAME}</a></strong>&nbsp;</span></td>
    <!-- END table_browse_fields -->
  </tr>
  <!-- BEGIN table_browse_data -->
  <tr>
    <td class="row1"><span class="gensmall"><a href="{switch_table_browse.table_browse_data.TABLE_BROWSE_DELETE}">{L_TABLE_BROWSE_DELETE}</a>&nbsp;</span></td>
    <!-- BEGIN table_browse_data_field -->
    <td class="row1"><span class="gensmall">{switch_table_browse.table_browse_data.table_browse_data_field.TABLE_BROWSE_DATA}&nbsp;</span></td>
    <!-- END table_browse_data_field -->
  </tr>
  <!-- END table_browse_data -->
</table>
<br />
<!-- END switch_table_browse -->

<!-- BEGIN switch_table_structure -->
<table width="100%" cellspacing="0" cellpadding="0" border="1" class="forumline">
  <tr>
    <td colspan="7" align="center" class="catLeft"><span class="gen"><strong>{L_TABLE_STRUCTURE_TABLENAME}</strong></span></td>
  </tr>
  <tr>
    <td class="row1"><span class="genmed"><strong>&nbsp;</strong></span></td>
    <td class="row1"><span class="genmed"><strong>{L_TABLE_STRUCTURE_FIELD}</strong>&nbsp;</span></td>
    <td class="row1"><span class="genmed"><strong>{L_TABLE_STRUCTURE_TYPE}</strong>&nbsp;</span></td>
    <td class="row1"><span class="genmed"><strong>{L_TABLE_STRUCTURE_NULL}</strong>&nbsp;</span></td>
    <td class="row1"><span class="genmed"><strong>{L_TABLE_STRUCTURE_KEY}</strong>&nbsp;</span></td>
    <td class="row1"><span class="genmed"><strong>{L_TABLE_STRUCTURE_DEFAULT}</strong>&nbsp;</span></td>
    <td class="row1"><span class="genmed"><strong>{L_TABLE_STRUCTURE_EXTRA}</strong>&nbsp;</span></td>
  </tr>
  <!-- BEGIN actual_table_structure -->
  <tr>
    <td class="row1"><span class="gensmall"><a href="{switch_table_structure.actual_table_structure.TABLE_STRUCTURE_DROP}">{L_TABLE_STRUCTURE_DROP}</a>&nbsp;</span></td>
    <td class="row1"><span class="gensmall">{switch_table_structure.actual_table_structure.TABLE_STRUCTURE_FIELD}&nbsp;</span></td>
    <td class="row1"><span class="gensmall">{switch_table_structure.actual_table_structure.TABLE_STRUCTURE_TYPE}&nbsp;</span></td>
    <td class="row1"><span class="gensmall">{switch_table_structure.actual_table_structure.TABLE_STRUCTURE_NULL}&nbsp;</span></td>
    <td class="row1"><span class="gensmall">{switch_table_structure.actual_table_structure.TABLE_STRUCTURE_KEY}&nbsp;</span></td>
    <td class="row1"><span class="gensmall">{switch_table_structure.actual_table_structure.TABLE_STRUCTURE_DEFAULT}&nbsp;</span></td>
    <td class="row1"><span class="gensmall">{switch_table_structure.actual_table_structure.TABLE_STRUCTURE_EXTRA}&nbsp;</span></td>
  </tr>
  <!-- END actual_table_structure -->
</table>
<br />
<!-- END switch_table_structure -->

<table width="100%" cellspacing="0" cellpadding="0" border="1" class="forumline">
  <tr>
    <td colspan="12" align="center" class="catLeft"><span class="gen"><strong>{TABLE_TITLE}</strong></span></td>
  </tr>
  <tr>
    <td class="row1"><span class="genmed"><strong>{L_TABLE_NAME}</strong>&nbsp;</span></td>
    <td colspan="6" align="center" class="row1"><span class="genmed"><strong>{L_TABLE_ACTIONS}</strong>&nbsp;</span></td>
    <td class="row1"><span class="genmed"><strong>{L_TABLE_TYPE}</strong>&nbsp;</span></td>
    <td class="row1"><span class="genmed"><strong>{L_TABLE_ROWS}</strong>&nbsp;</span></td>
    <td class="row1"><span class="genmed"><strong>{L_TABLE_DATA_LENGTH}</strong>&nbsp;</span></td>
    <td class="row1"><span class="genmed"><strong>{L_TABLE_OPTIMIZATION_LEVEL}</strong>&nbsp;</span></td>
    <td class="row1"><span class="genmed">&nbsp;</span></td>
  </tr>
<!-- BEGIN table_list -->
  <tr>
    <td class="row1"><span class="gensmall">{table_list.TABLE_NAME}&nbsp;</span></td>
    <td class="row1"><span class="gensmall"><a href="{table_list.TABLE_STRUCTURE}">{L_TABLE_STRUCTURE}</a>&nbsp;</span></td>
    <td class="row1"><span class="gensmall"><a href="{table_list.TABLE_BROWSE}">{L_TABLE_BROWSE}</a>&nbsp;</span></td>
    <td class="row1"><span class="gensmall"><a href="{table_list.TABLE_OPTIMIZE}">{L_TABLE_OPTIMIZE}</a>&nbsp;</span></td>
    <td class="row1"><span class="gensmall"><a href="{table_list.TABLE_REPAIR}">{L_TABLE_REPAIR}</a>&nbsp;</span></td>
    <td class="row1"><span class="gensmall"><a href="{table_list.TABLE_EMPTY}">{L_TABLE_EMPTY}</a>&nbsp;</span></td>
    <td class="row1"><span class="gensmall"><a href="{table_list.TABLE_DROP}">{L_TABLE_DROP}</a>&nbsp;</span></td>
    <td class="row1"><span class="gensmall">{table_list.TABLE_TYPE}&nbsp;</span></td>
    <td class="row1"><span class="gensmall">{table_list.TABLE_ROWS}&nbsp;</span></td>
    <td class="row1"><span class="gensmall">{table_list.TABLE_DATA_LENGTH}&nbsp;</span></td>
    <td class="row1"><span class="gensmall">{table_list.TABLE_OPTIMIZATION_LEVEL}%&nbsp;</span></td>
    <td class="row1"><input type="checkbox" name="with_selected_table_list[]" value="{table_list.TABLE_NAME}" /></td>
  </tr>
<!-- END table_list -->
</table>

</form>

<table width="100%" cellspacing="0" border="0" cellpadding="0">
  <tr>
    <td align="center"><span class="copyright">{COPYRIGHT}</span></td>
  </tr>
</table>